<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Mobile_Should_have_ten_digits</fullName>
    <active>true</active>
    <errorConditionFormula>LEN( MobilePhone ) != 10 &amp;&amp; LEN( MobilePhone ) != 0</errorConditionFormula>
    <errorDisplayField>MobilePhone</errorDisplayField>
    <errorMessage>Please enter 10 digit mobile number</errorMessage>
</ValidationRule>
